{
  "compileOnSave": false,
  "compilerOptions": {
    "preserveSymlinks": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "admin-panel-generator": [
        "dist/admin-panel-generator"
      ],
      "@admin-panel/core": [
        "dist/admin-panel/core/admin-panel-core",
        "dist/admin-panel/core"
      ],
      "@admin-panel/components": [
        "dist/admin-panel/components/admin-panel-components",
        "dist/admin-panel/components"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}