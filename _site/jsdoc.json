  "source": {
    "include": "projects/playground-app/src",
    "includePattern": ".ts$",
    "excludePattern": "(node_modules/|jdocs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceTrue": true
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/"
  }
}